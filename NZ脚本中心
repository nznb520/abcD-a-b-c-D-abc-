-- 整合的悬浮球功能 - 使用您提供的代码替换原有悬浮球
local CLOSED_IMAGE_ID = "123947581167369"  -- 关闭状态图片
local OPENED_IMAGE_ID = "108010245085057"  -- 打开状态图片

-- 创建悬浮球
local FloatingBall = Instance.new("ImageButton")
FloatingBall.Name = "FloatingBall"
FloatingBall.Size = UDim2.new(0, 80, 0, 80)
FloatingBall.Position = UDim2.new(0.5, -40, 0.8, -40)  -- 屏幕底部中央
FloatingBall.AnchorPoint = Vector2.new(0.5, 0.5)
FloatingBall.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
FloatingBall.BackgroundTransparency = 1
FloatingBall.Image = "rbxassetid://" .. CLOSED_IMAGE_ID
FloatingBall.ScaleType = Enum.ScaleType.Fit
FloatingBall.Visible = false
FloatingBall.Parent = ScreenGui

-- 添加圆角效果
local FloatingBallCorner = Instance.new("UICorner")
FloatingBallCorner.CornerRadius = UDim.new(1, 0)
FloatingBallCorner.Parent = FloatingBall

-- 悬浮球边框效果
local FloatingBallStroke = Instance.new("UIStroke")
FloatingBallStroke.Color = COLOR_SCHEME.ACCENT_PURPLE
FloatingBallStroke.Thickness = 2
FloatingBallStroke.Parent = FloatingBall

-- 悬浮球状态变量
local isScriptEnabled = false
local isDragging = false
local dragStartPosition = Vector2.new(0, 0)
local ballStartPosition = Vector2.new(0, 0)
local longPressTime = 0.5  -- 长按时间阈值（秒）
local longPressTimer = 0
local isLongPressing = false

-- 更新图片函数
local function updateBallImage()
    if isScriptEnabled then
        FloatingBall.Image = "rbxassetid://" .. OPENED_IMAGE_ID
    else
        FloatingBall.Image = "rbxassetid://" .. CLOSED_IMAGE_ID
    end
end

-- 平滑移动函数
local function smoothMoveTo(position)
    local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
    local tween = TweenService:Create(FloatingBall, tweenInfo, {Position = position})
    tween:Play()
end

-- 输入开始处理
FloatingBall.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then
        isDragging = true
        dragStartPosition = Vector2.new(input.Position.X, input.Position.Y)
        ballStartPosition = Vector2.new(FloatingBall.Position.X.Offset, FloatingBall.Position.Y.Offset)
        longPressTimer = 0
        isLongPressing = false
        
        -- 开始检测长按
        local longPressThread = coroutine.create(function()
            while isDragging and longPressTimer < longPressTime do
                wait(0.1)
                longPressTimer = longPressTimer + 0.1
            end
            
            if isDragging and longPressTimer >= longPressTime then
                isLongPressing = true
            end
        end)
        coroutine.resume(longPressThread)
    end
end)

-- 输入改变处理（拖拽）
FloatingBall.InputChanged:Connect(function(input)
    if isDragging and isLongPressing and (input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseMovement) then
        local currentPosition = Vector2.new(input.Position.X, input.Position.Y)
        local delta = currentPosition - dragStartPosition
        
        local newX = ballStartPosition.X + delta.X
        local newY = ballStartPosition.Y + delta.Y
        
        -- 限制在屏幕范围内
        newX = math.max(0, math.min(newX, ScreenGui.AbsoluteSize.X - FloatingBall.AbsoluteSize.X))
        newY = math.max(0, math.min(newY, ScreenGui.AbsoluteSize.Y - FloatingBall.AbsoluteSize.Y))
        
        FloatingBall.Position = UDim2.new(0, newX, 0, newY)
    end
end)

-- 输入结束处理
FloatingBall.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then
        isDragging = false
        
        if not isLongPressing then
            -- 点击事件：切换脚本状态
            isScriptEnabled = not isScriptEnabled
            updateBallImage()
            
            -- 切换主界面显示/隐藏
            if isScriptEnabled then
                MainFrame.Visible = true
                FloatingBall.BackgroundColor3 = COLOR_SCHEME.ACCENT_BLUE
            else
                MainFrame.Visible = false
                FloatingBall.BackgroundColor3 = COLOR_SCHEME.ACCENT_PURPLE
            end
        end
        
        isLongPressing = false
    end
end)

-- 屏幕尺寸变化时重新定位
ScreenGui:GetPropertyChangedSignal("AbsoluteSize"):Connect(function()
    if not isDragging then
        local currentPos = FloatingBall.Position
        local screenSize = ScreenGui.AbsoluteSize
        local ballSize = FloatingBall.AbsoluteSize
        
        -- 确保悬浮球不会超出屏幕边界
        local newX = math.max(0, math.min(currentPos.X.Offset, screenSize.X - ballSize.X))
        local newY = math.max(0, math.min(currentPos.Y.Offset, screenSize.Y - ballSize.Y))
        
        if newX ~= currentPos.X.Offset or newY ~= currentPos.Y.Offset then
            smoothMoveTo(UDim2.new(0, newX, 0, newY))
        end
    end
end)

