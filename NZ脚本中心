-- ================ åœ°å›¾é€è§† ================
xrayEnabled = false
function xray()
    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA("BasePart") and not v.Parent:FindFirstChildWhichIsA("Humanoid") and
            not v.Parent.Parent:FindFirstChildWhichIsA("Humanoid") then
            v.LocalTransparencyModifier = xrayEnabled and 0.5 or 0
        end
    end
end

local featureToggle = TabHandles.YI:Toggle({
    Title = "åœ°å›¾é€è§†",
    Desc = "Xray",
    Value = false,
    Callback = function(state)
        if state then
            xrayEnabled = true
            xray()
        else
            xrayEnabled = false
            xray()
        end
        WindUI:Notify({
            Title = "HB FXMï¼š",
            Content = state and "åœ°å›¾é€è§†å·²å¼€å¯ï¼Œè‹¥ä»ä¸æ¸…æ¥šå¯å¼€å¯å…¶ä»–è§†è§‰åŠŸèƒ½" or
                "åœ°å›¾é€è§†å·²å…³é—­",
            Icon = state and "check" or "x",
            Duration = 2
        })
    end
})

