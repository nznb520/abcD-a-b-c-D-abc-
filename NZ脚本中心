-- å˜é‡
local isDragging = false
local dragStartPos = Vector2.new(0, 0)
local frameStartPos = UDim2.new(0, 0, 0, 0)
local isRestoreDragging = false
local restoreStartPos = Vector2.new(0, 0)
local buttonStartPos = UDim2.new(0, 0, 0, 0)
local espEnabled = false
local teamDetectEnabled = false
local espHighlights = {}
local correctKey = "nznb666"

-- å¤åˆ¶ç¾¤å·åŠŸèƒ½
CopyButton.MouseButton1Click:Connect(function()
    local clipboard = setclipboard or toclipboard or set_clipboard or (Clipboard and Clipboard.set)
    if clipboard then
        clipboard("813128648")
        StatusText.Text = "ç¾¤å·å·²å¤åˆ¶åˆ°å‰ªè´´æ¿"
    else
        StatusText.Text = "æ— æ³•å¤åˆ¶ï¼Œè¯·æ‰‹åŠ¨å¤åˆ¶ç¾¤å·"
    end
end)

-- éªŒè¯å¡å¯†åŠŸèƒ½
VerifyButton.MouseButton1Click:Connect(function()
    if KeyInput.Text == correctKey then
        KeyFrame:Destroy()
        MainFrame.Visible = true
        StatusText.Text = "éªŒè¯æˆåŠŸï¼Œæ¬¢è¿ä½¿ç”¨"
    else
        KeyInput.Text = ""
        KeyInput.PlaceholderText = "å¡å¯†é”™è¯¯ï¼Œè¯·é‡æ–°è¾“å…¥"
    end
end)

-- æœ€å°åŒ–æŒ‰é’®åŠŸèƒ½
MinimizeButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = false
    RestoreButton.Visible = true
    StatusText.Text = "è„šæœ¬å·²æœ€å°åŒ–"
end)

-- æ¢å¤æŒ‰é’®åŠŸèƒ½
RestoreButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = true
    RestoreButton.Visible = false
    StatusText.Text = "è„šæœ¬å·²æ¢å¤"
end)

-- æ‹–åŠ¨ä¸»çª—å£
TitleBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        isDragging = true
        dragStartPos = Vector2.new(input.Position.X, input.Position.Y)
        frameStartPos = MainFrame.Position
    end
end)

-- æ‹–åŠ¨æ¢å¤æŒ‰é’®
RestoreButton.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        isRestoreDragging = true
        restoreStartPos = Vector2.new(input.Position.X, input.Position.Y)
        buttonStartPos = RestoreButton.Position
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        if isDragging then
            local delta = Vector2.new(input.Position.X, input.Position.Y) - dragStartPos
            MainFrame.Position = UDim2.new(frameStartPos.X.Scale, frameStartPos.X.Offset + delta.X, 
                                          frameStartPos.Y.Scale, frameStartPos.Y.Offset + delta.Y)
        elseif isRestoreDragging then
            local delta = Vector2.new(input.Position.X, input.Position.Y) - restoreStartPos
            RestoreButton.Position = UDim2.new(buttonStartPos.X.Scale, buttonStartPos.X.Offset + delta.X, 
                                              buttonStartPos.Y.Scale, buttonStartPos.Y.Offset + delta.Y)
        end
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        isDragging = false
        isRestoreDragging = false
    end
end)

-- å¯¼èˆªæŒ‰é’®åŠŸèƒ½
GeneralButton.MouseButton1Click:Connect(function()
    GeneralFrame.Visible = true
    ESPFrame.Visible = false
    GeneralButton.BackgroundColor3 = Color3.new(0.15, 0.15, 0.3)
    ESPButton.BackgroundColor3 = Color3.new(0.1, 0.1, 0.2)
    StatusText.Text = "åˆ‡æ¢åˆ°é€šç”¨åŠŸèƒ½"
end)

ESPButton.MouseButton1Click:Connect(function()
    GeneralFrame.Visible = false
    ESPFrame.Visible = true
    GeneralButton.BackgroundColor3 = Color3.new(0.1, 0.1, 0.2)
    ESPButton.BackgroundColor3 = Color3.new(0.15, 0.15, 0.3)
    StatusText.Text = "åˆ‡æ¢åˆ°ESPåŠŸèƒ½"
end)

-- æ›´æ–°ESPæ˜¾ç¤º
local function updateESP()
    -- æ¸…é™¤ç°æœ‰é«˜äº®
    for _, highlight in pairs(espHighlights) do
        highlight:Destroy()
    end
    espHighlights = {}
    
    if not espEnabled then return end
    
    -- ä¸ºæ‰€æœ‰ç©å®¶æ·»åŠ é«˜äº®
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character then
            local highlight = Instance.new("Highlight")
            highlight.Parent = player.Character
            
            -- è®¾ç½®é¢œè‰²
            if teamDetectEnabled then
                -- é˜Ÿä¼æ£€æµ‹å¼€å¯æ—¶ï¼Œé˜Ÿå‹ç»¿è‰²ï¼Œæ•Œäººçº¢è‰²
                if player.Team == LocalPlayer.Team then
                    highlight.FillColor = Color3.new(0, 1, 0)  -- é˜Ÿå‹ç»¿è‰²
                    highlight.OutlineColor = Color3.new(0, 0.8, 0)
                else
                    highlight.FillColor = Color3.new(1, 0, 0)  -- æ•Œäººçº¢è‰²
                    highlight.OutlineColor = Color3.new(0.8, 0, 0)
                end
            else
                -- é˜Ÿä¼æ£€æµ‹å…³é—­æ—¶ï¼Œç»Ÿä¸€ç™½è‰²
                highlight.FillColor = Color3.new(1, 1, 1)  -- ç™½è‰²
                highlight.OutlineColor = Color3.new(0.8, 0.8, 0.8)
            end
            
            table.insert(espHighlights, highlight)
        end
    end
end

-- ESPå¼€å…³åŠŸèƒ½
ESPToggle.MouseButton1Click:Connect(function()
    espEnabled = not espEnabled
    
    if espEnabled then
        ESPToggle.Text = "å…³é—­ESP"
        StatusText.Text = "ESPå·²å¼€å¯"
    else
        ESPToggle.Text = "å¼€å¯ESP"
        StatusText.Text = "ESPå·²å…³é—­"
    end
    
    updateESP()
end)

-- é˜Ÿä¼æ£€æµ‹å¼€å…³åŠŸèƒ½
TeamDetectToggle.MouseButton1Click:Connect(function()
    teamDetectEnabled = not teamDetectEnabled
    
    if teamDetectEnabled then
        TeamDetectToggle.Text = "å…³é—­é˜Ÿä¼æ£€æµ‹"
        StatusText.Text = "é˜Ÿä¼æ£€æµ‹å·²å¼€å¯"
    else
        TeamDetectToggle.Text = "å¼€å¯é˜Ÿä¼æ£€æµ‹"
        StatusText.Text = "é˜Ÿä¼æ£€æµ‹å·²å…³é—­"
    end
    
    if espEnabled then
        updateESP()
    end
end)

-- ç©å®¶åŠ å…¥/ç¦»å¼€æ—¶æ›´æ–°ESP
Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function(character)
        if espEnabled then
            updateESP()
        end
    end)
end)

Players.PlayerRemoving:Connect(function(player)
    if espEnabled then
        updateESP()
    end
end)

-- è§’è‰²é‡ç”Ÿæ—¶æ›´æ–°ESP
LocalPlayer.CharacterAdded:Connect(function(character)
    if espEnabled then
        updateESP()
    end
end)

-- é˜Ÿä¼å˜åŒ–æ—¶æ›´æ–°ESP
LocalPlayer:GetPropertyChangedSignal("Team"):Connect(function()
    if espEnabled and teamDetectEnabled then
        updateESP()
    end
end)

-- å…³é—­æŒ‰é’®åŠŸèƒ½
CloseButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = false
    StatusText.Text = "ç•Œé¢å·²å…³é—­"
end)

-- è¾¹æ¡†é¢œè‰²å˜åŒ–æ•ˆæœ
spawn(function()
    while RestoreGlow and RestoreGlow.Parent do
        for i = 0, 1, 0.01 do
            if not RestoreGlow or not RestoreGlow.Parent then break end
            RestoreGlow.ImageColor3 = Color3.fromHSV(i, 0.8, 1)
            task.wait(0.05)
        end
    end
end)
