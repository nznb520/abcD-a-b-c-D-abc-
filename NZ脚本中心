-- å˜é‡
local isDragging = false
local dragStartPos = Vector2.new(0, 0)
local frameStartPos = UDim2.new(0, 0, 0, 0)
local activeFunctions = {}
local correctKey = "nznb666"

-- å¤åˆ¶ç¾¤å·åŠŸèƒ½
CopyButton.MouseButton1Click:Connect(function()
    local clipboard = setclipboard or toclipboard or set_clipboard or (Clipboard and Clipboard.set)
    if clipboard then
        clipboard("813128648")
        StatusText.Text = "ç¾¤å·å·²å¤åˆ¶åˆ°å‰ªè´´æ¿"
    else
        StatusText.Text = "æ— æ³•å¤åˆ¶ï¼Œè¯·æ‰‹åŠ¨å¤åˆ¶ç¾¤å·"
    end
end)

-- éªŒè¯å¡å¯†åŠŸèƒ½
VerifyButton.MouseButton1Click:Connect(function()
    if KeyInput.Text == correctKey then
        KeyFrame.Visible = false
        WelcomeFrame.Visible = true
        
        -- åŠ è½½åŠ¨ç”»æ•ˆæœ
        spawn(function()
            for i = 0, 400, 2 do
                if not LoadingBar or not LoadingBar.Parent then break end
                LoadingBar.Size = UDim2.new(0, i, 0, 5)
                task.wait()
            end
        end)
        
        -- å»¶è¿Ÿæ˜¾ç¤ºä¸»ç•Œé¢
        task.delay(3, function()
            WelcomeFrame:Destroy()
            MainFrame.Visible = true
        end)
    else
        KeyInput.Text = ""
        KeyInput.PlaceholderText = "å¡å¯†é”™è¯¯ï¼Œè¯·é‡æ–°è¾“å…¥"
    end
end)

-- æ‹–åŠ¨ä¸»çª—å£
TitleBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        isDragging = true
        dragStartPos = Vector2.new(input.Position.X, input.Position.Y)
        frameStartPos = MainFrame.Position
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement and isDragging then
        local delta = Vector2.new(input.Position.X, input.Position.Y) - dragStartPos
        MainFrame.Position = UDim2.new(frameStartPos.X.Scale, frameStartPos.X.Offset + delta.X, 
                                      frameStartPos.Y.Scale, frameStartPos.Y.Offset + delta.Y)
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        isDragging = false
    end
end)

-- å¿«é€Ÿæ—‹è½¬åŠŸèƒ½
FastSpinButton.MouseButton1Click:Connect(function()
    StatusText.Text = "å¿«é€Ÿæ—‹è½¬å·²æ¿€æ´»"
    
    local function startFastSpin(character)
        local humanoid = character:WaitForChild("Humanoid")
        humanoid.AutoRotate = false
        
        local connection = game:GetService("RunService").Heartbeat:Connect(function()
            if humanoid and humanoid.Parent then
                humanoid.RootPart.CFrame = humanoid.RootPart.CFrame * CFrame.Angles(0, math.rad(50), 0)
            end
        end)
        
        table.insert(activeFunctions, connection)
    end
    
    LocalPlayer.CharacterAdded:Connect(startFastSpin)
    
    if LocalPlayer.Character then
        startFastSpin(LocalPlayer.Character)
    end
end)

-- æé€Ÿæ—‹è½¬åŠŸèƒ½
SuperSpinButton.MouseButton1Click:Connect(function()
    StatusText.Text = "æé€Ÿæ—‹è½¬å·²æ¿€æ´»"
    
    local function startSuperSpin(character)
        local humanoid = character:WaitForChild("Humanoid")
        humanoid.AutoRotate = false
        
        local connection = game:GetService("RunService").Heartbeat:Connect(function()
            if humanoid and humanoid.Parent then
                humanoid.RootPart.CFrame = humanoid.RootPart.CFrame * CFrame.Angles(0, math.rad(100), 0)
            end
        end)
        
        table.insert(activeFunctions, connection)
    end
    
    LocalPlayer.CharacterAdded:Connect(startSuperSpin)
    
    if LocalPlayer.Character then
        startSuperSpin(LocalPlayer.Character)
    end
end)

-- æ— é™è·³åŠŸèƒ½
InfiniteJumpButton.MouseButton1Click:Connect(function()
    StatusText.Text = "æ— é™è·³è·ƒå·²æ¿€æ´»"
    
    local function startInfiniteJump(character)
        local humanoid = character:WaitForChild("Humanoid")
        
        local connection = UserInputService.JumpRequest:Connect(function()
            humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end)
        
        table.insert(activeFunctions, connection)
    end
    
    LocalPlayer.CharacterAdded:Connect(startInfiniteJump)
    
    if LocalPlayer.Character then
        startInfiniteJump(LocalPlayer.Character)
    end
end)

-- é£è¡ŒåŠŸèƒ½
FlightButton.MouseButton1Click:Connect(function()
    StatusText.Text = "æ­£åœ¨åŠ è½½é£è¡Œè„šæœ¬..."
    loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-Invinicible-Flight-R15-45414"))()
end)

