    Title = "åŠ¨ä½œ",
    Desc = "æ”¯æŒR15ä½“å‹",
    Locked = false,
    Callback = function()
       loadstring(game:HttpGet("https://pastebin.com/raw/Zj4NnKs6"))()

WindUI:Notify({
    Title = "é€šçŸ¥",
    Content = "åŠ è½½æˆåŠŸ",
    Duration = 1, -- 3 seconds
    Icon = "layout-grid",
})       
       
    end
