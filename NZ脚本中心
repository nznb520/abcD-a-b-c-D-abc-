-- 变量
local isDragging = false
local dragStartPos = Vector2.new(0, 0)
local frameStartPos = UDim2.new(0, 0, 0, 0)
local isRestoreDragging = false
local restoreStartPos = Vector2.new(0, 0)
local buttonStartPos = UDim2.new(0, 0, 0, 0)
local espEnabled = false
local teamDetectEnabled = false
local drawEnabled = false
local espHighlights = {}
local originalJumpHeight = 7.2
local originalWalkSpeed = 16
local originalFogEnabled = true
local originalFogStart = 0
local originalFogEnd = 1000
local correctKey = "nznb666"

-- 复制群号功能
CopyButton.MouseButton1Click:Connect(function()
    local clipboard = setclipboard or toclipboard or set_clipboard or (Clipboard and Clipboard.set)
    if clipboard then
        clipboard("813128648")
        StatusText.Text = "群号已复制到剪贴板"
    else
        StatusText.Text = "无法复制，请手动复制群号"
    end
end)

-- 验证卡密功能
VerifyButton.MouseButton1Click:Connect(function()
    if KeyInput.Text == correctKey then
        KeyFrame:Destroy()
        MainFrame.Visible = true
        StatusText.Text = "验证成功，欢迎使用"
    else
        KeyInput.Text = ""
        KeyInput.PlaceholderText = "卡密错误，请重新输入"
    end
end)

-- 最小化按钮功能
MinimizeButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = false
    RestoreButton.Visible = true
    StatusText.Text = "脚本已最小化"
end)

-- 恢复按钮功能
NameButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = true
    RestoreButton.Visible = false
    StatusText.Text = "脚本已恢复"
end)

-- 拖动主窗口
TitleBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        isDragging = true
        dragStartPos = Vector2.new(input.Position.X, input.Position.Y)
        frameStartPos = MainFrame.Position
    end
end)

-- 拖动恢复按钮
DragHandle.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        isRestoreDragging = true
        restoreStartPos = Vector2.new(input.Position.X, input.Position.Y)
        buttonStartPos = RestoreButton.Position
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        if isDragging then
            local delta = Vector2.new(input.Position.X, input.Position.Y) - dragStartPos
            MainFrame.Position = UDim2.new(frameStartPos.X.Scale, frameStartPos.X.Offset + delta.X, 
                                          frameStartPos.Y.Scale, frameStartPos.Y.Offset + delta.Y)
        elseif isRestoreDragging then
            local delta = Vector2.new(input.Position.X, input.Position.Y) - restoreStartPos
            RestoreButton.Position = UDim2.new(buttonStartPos.X.Scale, buttonStartPos.X.Offset + delta.X, 
                                              buttonStartPos.Y.Scale, buttonStartPos.Y.Offset + delta.Y)
        end
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        isDragging = false
        isRestoreDragging = false
    end
end)

-- 导航按钮功能
DevTeamButton.MouseButton1Click:Connect(function()
    DevTeamFrame.Visible = true
    GeneralFrame.Visible = false
    ESPFrame.Visible = false
    OtherScriptsFrame.Visible = false
    DevTeamButton.BackgroundColor3 = Color3.new(0.15, 0.15, 0.3)
    GeneralButton.BackgroundColor3 = Color3.new(0.1, 0.1, 0.2)
    ESPButton.BackgroundColor3 = Color3.new(0.1, 0.1, 0.2)
    OtherScriptsButton.BackgroundColor3 = Color3.new(0.1, 0.1, 0.2)
    StatusText.Text = "切换到开发团队信息"
end)

GeneralButton.MouseButton1Click:Connect(function()
    DevTeamFrame.Visible = false
    GeneralFrame.Visible = true
    ESPFrame.Visible = false
    OtherScriptsFrame.Visible = false
    DevTeamButton.BackgroundColor3 = Color3.new(0.1, 0.1, 0.2)
    GeneralButton.BackgroundColor3 = Color3.new(0.15, 0.15, 0.3)
    ESPButton.BackgroundColor3 = Color3.new(0.1, 0.1, 0.2)
    OtherScriptsButton.BackgroundColor3 = Color3.new(0.1, 0.1, 0.2)
    StatusText.Text = "切换到通用功能"
end)

ESPButton.MouseButton1Click:Connect(function()
    DevTeamFrame.Visible = false
    GeneralFrame.Visible = false
    ESPFrame.Visible = true
    OtherScriptsFrame.Visible = false
    DevTeamButton.BackgroundColor3 = Color3.new(0.1, 0.1, 0.2)
    GeneralButton.BackgroundColor3 = Color3.new(0.1, 0.1, 0.2)
    ESPButton.BackgroundColor3 = Color3.new(0.15, 0.15, 0.3)
    OtherScriptsButton.BackgroundColor3 = Color3.new(0.1, 0.1, 0.2)
    StatusText.Text = "切换到ESP功能"
end)

OtherScriptsButton.MouseButton1Click:Connect(function()
    DevTeamFrame.Visible = false
    GeneralFrame.Visible = false
    ESPFrame.Visible = false
    OtherScriptsFrame.Visible = true
    DevTeamButton.BackgroundColor3 = Color3.new(0.1, 0.1, 0.2)
    GeneralButton.BackgroundColor3 = Color3.new(0.1, 0.1, 0.2)
    ESPButton.BackgroundColor3 = Color3.new(0.1, 0.1, 0.2)
    OtherScriptsButton.BackgroundColor3 = Color3.new(0.15, 0.15, 0.3)
    StatusText.Text = "切换到其他脚本"
end)

-- 更新ESP显示
local function updateESP()
    -- 清除现有高亮
    for _, highlight in pairs(espHighlights) do
        highlight:Destroy()
    end
    espHighlights = {}
    
    if not espEnabled then return end
    
    -- 为所有玩家添加高亮
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character then
            local highlight = Instance.new("Highlight")
            highlight.Parent = player.Character
            
            -- 设置颜色
            if teamDetectEnabled then
                -- 队伍检测开启时，队友绿色，敌人红色
                if player.Team == LocalPlayer.Team then
                    highlight.FillColor = Color3.new(0, 1, 0)  -- 队友绿色
                    highlight.OutlineColor = Color3.new(0, 0.8, 0)
                else
                    highlight.FillColor = Color3.new(1, 0, 0)  -- 敌人红色
                    highlight.OutlineColor = Color3.new(0.8, 0, 0)
                end
            else
                -- 队伍检测关闭时，统一白色
                highlight.FillColor = Color3.new(1, 1, 1)  -- 白色
                highlight.OutlineColor = Color3.new(0.8, 0.8, 0.8)
            end
            
            table.insert(espHighlights, highlight)
        end
    end
end

-- ESP开关功能
ESPToggle.MouseButton1Click:Connect(function()
    espEnabled = not espEnabled
    
    if espEnabled then
        ESPToggle.Text = "关闭ESP"
        StatusText.Text = "ESP已开启"
    else
        ESPToggle.Text = "开启ESP"
        StatusText.Text = "ESP已关闭"
    end
    
    updateESP()
end)

-- 队伍检测开关功能
TeamDetectToggle.MouseButton1Click:Connect(function()
    teamDetectEnabled = not teamDetectEnabled
    
    if teamDetectEnabled then
        TeamDetectToggle.Text = "关闭队伍检测"
        StatusText.Text = "队伍检测已开启"
    else
        TeamDetectToggle.Text = "开启队伍检测"
        StatusText.Text = "队伍检测已关闭"
    end
    
    if espEnabled then
        updateESP()
    end
end)

-- 绘制开关功能
DrawToggle.MouseButton1Click:Connect(function()
    drawEnabled = not drawEnabled
    
    if drawEnabled then
        DrawToggle.Text = "关闭绘制"
        StatusText.Text = "绘制已开启"
        
        if espEnabled then
            if teamDetectEnabled then
                -- 只绘制敌人
                for _, player in pairs(Players:GetPlayers()) do
                    if player ~= LocalPlayer and player.Character and player.Team ~= LocalPlayer.Team then
                        local line = Instance.new("LineHandleAdornment")
                        line.Name = "ESPLine"
                        line.Length = 5
                        line.Thickness = 0.2
                        line.Color3 = Color3.new(1, 0, 0)
                        line.Adornee = player.Character:FindFirstChild("HumanoidRootPart")
                        line.Parent = player.Character
                    end
                end
            else
                -- 绘制所有人
                for _, player in pairs(Players:GetPlayers()) do
                    if player ~= LocalPlayer and player.Character then
                        local line = Instance.new("LineHandleAdornment")
                        line.Name = "ESPLine"
                        line.Length = 5
                        line.Thickness = 0.2
                        line.Color3 = Color3.new(1, 1, 1)
                        line.Adornee = player.Character:FindFirstChild("HumanoidRootPart")
                        line.Parent = player.Character
                    end
                end
            end
        end
    else
        DrawToggle.Text = "开启绘制"
        StatusText.Text = "绘制已关闭"
        
        -- 清除所有线条
        for _, player in pairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and player.Character then
                local line = player.Character:FindFirstChild("ESPLine")
                if line then
                    line:Destroy()
                end
            end
        end
    end
end)

-- 修改跳跃高度
JumpHeightButton.MouseButton1Click:Connect(function()
    local newHeight = tonumber(JumpHeightInput.Text)
    if newHeight and newHeight > 0 then
        if LocalPlayer.Character then
            local humanoid = LocalPlayer.Character:FindFirstChild("Humanoid")
            if humanoid then
                originalJumpHeight = humanoid.JumpHeight
                humanoid.JumpHeight = newHeight
                StatusText.Text = "跳跃高度已修改为: " .. newHeight
            end
        else
            StatusText.Text = "没有找到角色"
        end
    else
        StatusText.Text = "请输入有效的跳跃高度"
    end
end)

-- 修改移动速度
MoveSpeedButton.MouseButton1Click:Connect(function()
    local newSpeed = tonumber(MoveSpeedInput.Text)
    if newSpeed and newSpeed > 0 then
        if LocalPlayer.Character then
            local humanoid = LocalPlayer.Character:FindFirstChild("Humanoid")
            if humanoid then
                originalWalkSpeed = humanoid.WalkSpeed
                humanoid.WalkSpeed = newSpeed
                StatusText.Text = "移动速度已修改为: " .. newSpeed
            end
        else
            StatusText.Text = "没有找到角色"
        end
    else
        StatusText.Text = "请输入有效的移动速度"
    end
end)

-- 夜视功能
NightVisionButton.MouseButton1Click:Connect(function()
    if Lighting then
        if Lighting.Ambient == Color3.new(1, 1, 1) then
            Lighting.Ambient = Color3.new(0.5, 0.5, 0.5)
            NightVisionButton.Text = "关闭夜视"
            StatusText.Text = "夜视已关闭"
        else
            Lighting.Ambient = Color3.new(1, 1, 1)
            NightVisionButton.Text = "开启夜视"
            StatusText.Text = "夜视已开启"
        end
    else
        StatusText.Text = "无法访问光照服务"
    end
end)

-- 一键去雾
RemoveFogButton.MouseButton1Click:Connect(function()
    if Lighting then
        if Lighting.FogEnd == 0 then
            -- 恢复原雾效果
            Lighting.FogEnd = originalFogEnd
            Lighting.FogStart = originalFogStart
            RemoveFogButton.Text = "一键去雾"
            StatusText.Text = "雾效果已恢复"
        else
            -- 保存原雾效果并去除
            originalFogEnd = Lighting.FogEnd
            originalFogStart = Lighting.FogStart
            Lighting.FogEnd = 0
            RemoveFogButton.Text = "恢复雾效果"
            StatusText.Text = "雾效果已去除"
        end
    else
        StatusText.Text = "无法访问光照服务"
    end
end)

